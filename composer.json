{
    "name": "softspring/doctrine-target-entity-resolver",
    "description": "This package provides a class that helps resolve target entities",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "phpstan/phpstan": "^1.9",
        "symfony/dependency-injection": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\DoctrineTargetEntityResolver\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
